{
	"presets": ["@babel/preset-react"],
	"plugins": [
		"@babel/plugin-syntax-import-assertions",
		["module-resolver", {
			"root": ["./src"],
			"alias": {
				"common": "./src/common",
				"server": "./src/server",
				"@/catalog": "./src/server/data/catalog/",
				"@/components": "./src/common/components/",
				"@/data": "./src/server/data",
				"@/pages": "./src/common/components/pages/",
				"@accounts": "./src/server/accounts",
				"@behavior": "./src/common/components/behavior/index",
				"@contexts": "./src/common/contexts",
				"@db": "./src/server/db",
				"@errors": "./src/common/errors",
				"@form": "./src/common/components/form/index",
				"@layout": "./src/common/components/layout/index",
				"@propTypes": "./src/common/propTypes/index",
				"@utils": "./src/common/utils/index",
				"@apiTypes": "./src/server/api-types"
			}
		}]
	]
}