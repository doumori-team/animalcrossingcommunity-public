    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "axios": "^1.8.4",
    "buffer-crc32": "^1.0.0",
    "compressorjs": "^1.2.1",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
