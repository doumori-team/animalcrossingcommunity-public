{
  "name": "animalcrossingcommunity",
  "version": "2.0.0",
  "private": true,
  "description": "Animal Crossing Community",
  "type": "module",
  "main": "src/server/server.js",
  "scripts": {
    "build": "ci/build.sh",
    "build-local": "react-router build --mode development",
    "build-local-prd": "react-router build",
    "build-watch": "chokidar \"src/**/*\" --initial -c \"npm run build-local\"",
    "watch": "chokidar \"src/**/*\" -c \"npm run build-local\"",
    "serve": "heroku local web",
    "update-init-scripts": "ci/update-init-scripts.sh",
    "lint": "node --max-old-space-size=4096 ./node_modules/eslint/bin/eslint.js src",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --fix src",
    "typecheck": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc",
    "test": "vitest",
    "test-server": "vitest tests/server",
    "test-client": "vitest tests/common",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
